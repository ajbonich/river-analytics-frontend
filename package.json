    "date-fns": "^2.0.0-alpha.34",
    "history": "^4.10.0",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.20",
    "node-sass": "^4.13.1",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-leaflet": "^3.0.5",
    "react-leaflet-markercluster": "^3.0.0-rc1",
    "react-material-ui-form-validator": "^2.0.9",
    "react-perfect-scrollbar": "^1.5.2",
    "react-quill": "^1.3.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.3.1",
    "react-scroll": "^1.8.1",
